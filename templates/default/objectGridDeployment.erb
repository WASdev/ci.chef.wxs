        <mapSet name="<%= @wxsMapSetName %>" numberOfPartitions="<%= @wxsMapSetNumberOfPartitions %>"
            developmentMode="<%= @wxsMapSetDevMode %>" minSyncReplicas="<%= @wxsMapSetMinSyncReplicas %>" maxSyncReplicas="<%= @wxsMapSetMaxSyncReplicas %>"
